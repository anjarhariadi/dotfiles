// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+shift+=",
    "command": "editor.action.smartSelect.grow",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+-",
    "command": "editor.action.smartSelect.shrink",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+.",
    "command": "editor.action.selectToBracket",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+right",
    "command": "editor.action.smartSelect.grow",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+left",
    "command": "editor.action.smartSelect.shrink",
    "when": "editorTextFocus"
  }
]
